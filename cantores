cartor = ['iza','jorge e matheus','fernando e sorocaba','thiaguinho']

cantores = []
palco = 0

for i in range (4):
  cantores.append(input('digite o nome dos cantores que ir√£o se apresentar'))
  if cantores[i] == cantor[i]:
    palco = palco + 1
print(palco)
cantores.remove(cantores[3])
novo_cantor = input('leonardo')
